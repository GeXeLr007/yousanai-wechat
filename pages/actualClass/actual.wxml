
<view class="appointment">
 <view class="app-title">实战课程</view>
  <view class="app-moduler">
  <block wx:for="{{actualUrls}}">
    <navigator url="url" class="mod-item">
      <view class="mod-signup">
        <image src="{{signupimg}}"></image>
      </view>
      <view class="mod-img">
        <image src="{{item.picture}}" class="mod-image"></image>
      </view>
      <view class="mod-title">
         <text>{{item.name}}</text>  
      </view>
      <view class="mod-info">
        <view class="mod-icon">
          <image src="{{iconsrc}}" class="icon"></image>
        </view>
        <view class="moinfont">{{item.studyCount}}人已申请</view>
      </view>
    </navigator>
    </block>
   </view>
</view>

